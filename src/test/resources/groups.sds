schema {
	type "group_tests"
	node { name "id" type "integer" value "[1..*]" }
	node {
		name "group_tests"
		node {
			name "man_man" occurs "1..*"
			node { type "id" }
			group {
				node { name "man1" type "string" }
				node { name "man2" type "string" }
			}
		}
		node {
			name "opt_man" occurs "1..*"
			node { type "id" }
			group {
				occurs "0..1"
				node { name "man1" type "string" }
				node { name "man2" type "string" }
			}
		}
		node {
			name "man_opt" occurs "1..*"
			node { type "id" }
			group {
				node { name "opt1" type "string" occurs "0..1" }
				node { name "opt2" type "string" occurs "0..1" }
			}
		}
		node {
			name "opt_opt" occurs "1..*"
			node { type "id" }
			group {
				occurs "0..1"
				node { name "opt1" type "string" occurs "0..1" }
				node { name "opt2" type "string" occurs "0..1" }
			}
		}
	}
}