schema {
	node {
		name "contacts"
		node {
			name "contact"  multiplicity "0..*"
			node { name "id" type "integer" value "(0..*)" }
			node { name "name" type "string" nullable "false" }
			node { name "phone" type "string" multiplicity "0..1" pattern "\\d{2,4}-\\d{6,8}" length "11" }
			node { name "birthdate" type "date" value "[1900-01-01..*)" multiplicity "0..1" }
			node { name "star" type "boolean" multiplicity "0..1" }
			node { name "icon" type "binary" multiplicity "0..1" }
			node { name "about" type "any" multiplicity "0..1" }
		}
	}
}